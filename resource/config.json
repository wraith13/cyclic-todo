{
    "applicationTitle": "Cyclic ToDo",
    "localDbPrefix": "cyclic-todo",
    "repositoryUrl": "https://github.com/wraith13/cyclic-todo/",
    "timeAccuracy": "1m",
    "standardDeviationRate": 1.5,
    "sleepStandardDeviationRate": 3.0,
    "granceTimespan": "1d",
    "maxHistories": 100,
    "maxGroupHistories": 3000,
    "maxRestAdjustTimespan": "3h",
    "maxShortTermTimespan": "3d",
    "maxMediumTermTimespan": "15d",
    "timespanPreset": [
        "4.5h", "21h", "9h", "3d", "30d", "5d", "3h", "12h",
        "18h", "21d", "45d", "4d", "7d", "6h", "2h", "1d",
        "14d", "180d", "240d", "2d", "15h", "1.5d", "2.5d", "10d",
        "60d"
    ],
    "tickScalePreset":[
        "1m", "5m", "10m", "30m",
        "1h", "3h", "6h", "12h",
        "1d", "7d", "30d",
        "0.25y", "1y", "10y"
    ],
    "timespanPresetMaxCount": 12,
    "timespanStandardScorePreset": [ 50, 40, 60, 30, 70 ],
    "timespanStandardScorePresetMaxCount": 5,
    "removedItemExpire": "30d",
    "removedItemDecayInterval": "6h",
    "autoTagSortOrder":
    [
        "@flash",
        "@pickup",
        "default",
        "@restriction"
    ]
